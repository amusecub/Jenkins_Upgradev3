pipeline
{
      agent any 
      stages
      {
            stage('init')
            {
                  steps
                  {
                        echo "This is init stage"
                  }
            }
            stage('build')
            {
                  steps
                  {
                        echo "This is build stage"
                  }
            }
            stage('test')
            {
                  steps
                  {
                        echo "This is test stage"
                  }
            }
            stage('Deploy')
            {
                  steps
                  {
                        echo "This is deploy stage"
                  }
            }
      }
}